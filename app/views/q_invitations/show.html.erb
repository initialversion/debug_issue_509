<div class="page-header">
  <h3>Q Invitation #<%= @q_invitation.id %></h3>
</div>

<div class="row">
  <div class="col-md-12 mb-2">
    <dl class="dl-horizontal">
      <dt>Sender</dt>
      <dd><%= @q_invitation.sender_id %></dd>

      <dt>Recipient</dt>
      <dd><%= @q_invitation.recipient_id %></dd>

      <dt>Status</dt>
      <dd><%= @q_invitation.status %></dd>

      <dt>Queue</dt>
      <dd><%= @q_invitation.queue_id %></dd>

    </dl>

    <div class="btn-group btn-group-justified">
      <a href="/q_invitations" class="btn btn-primary">
        Back
      </a>
      <a href="/q_invitations/<%= @q_invitation.id %>/edit" class="btn btn-warning">
        Edit
      </a>
      <a href="/delete_q_invitation/<%= @q_invitation.id %>" class="btn btn-danger" rel="nofollow">
        Delete
      </a>
    </div>
  </div>
</div>
